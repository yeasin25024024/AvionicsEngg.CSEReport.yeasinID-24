#include <stdio.h>

int main() 
{
    int r,c;

    printf("Enter number of rows: ");
    scanf("%d", &r);

    printf("Enter number of columns: ");
    scanf("%d", &c);

    int matrix[3][2];

    // Input matrix elements
    printf("Enter elements of the matrix: \n");
    for (int i = 0; i < r; i++) 
    {
        for (int j = 0; j < c; j++)
        {
            scanf("%d", &matrix[i][j]);
        }
    }
    return 0;
}
